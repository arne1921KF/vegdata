citHeader("To cite package 'vegdata' in publications use:")

citEntry(entry="Manual",
	title ="vegdata: Vegetation Data Access and Evaluation",
	author = personList(as.person("Florian Jansen")),
	year = 2009,
	note = paste("R package version", packageDescription("vegdata")$Version),
	url = "http://geobot.botanik.uni-greifswald.de/portal/",

	textVersion =
	paste("Jansen, F. (2009). ", 
	"vegdata: Vegetation Data Access and Evaluation ", 
	"version ", packageDescription("vegdata")$Version, 
 	". http://geobot.botanik.uni-greifswald.de/portal", sep="")
)
 
